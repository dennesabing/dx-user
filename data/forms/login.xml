<?xml version="1.0" encoding="UTF-8"?>
<config>
	<form>
		<name>FormName</name>
		<attributes>
			<method>post</method>
		</attributes>
		<fieldset></fieldset>
		<fieldsets>
			<fsMain>
				<name>fsMain</name>
				<type>Zend\Form\Fieldset</type>
				<options>
					<legend></legend>
				</options>
			</fsMain>
		</fieldsets>
		<elements>
			<csrf>
				<type>Zend\Form\Element\Csrf</type>
			</csrf>
			<email>
				<type>Zend\Form\Element\Text</type>
				<fieldset>fsMain</fieldset>
				<options>
					<label>Email Address</label>
				</options>
			</email>
			<password>
				<type>Zend\Form\Element\Password</type>
				<fieldset>fsMain</fieldset>
				<options>
					<label>Password</label>
				</options>
			</password>
			<submitBtn>
				<type>Zend\Form\Element\Submit</type>
				<options>
					<label>Submit</label>
					<primary>1</primary>
				</options>
				<attributes>
					<value>Submit</value>
				</attributes>
			</submitBtn>
		</elements>
		<inputFilter>
			<fieldsets>
				<fsMain>
					<password>
						<name>password</name>
						<required>1</required>
						<validators>
							<not_empty>
								<options>
									<messages>
										<isEmpty>Required.</isEmpty>
									</messages>
								</options>
							</not_empty>
						</validators>
					</password>
					<email>
						<name>email</name>
						<required>1</required>
						<validators>
							<not_empty>
								<options>
									<messages>
										<isEmpty>Required.</isEmpty>
									</messages>
								</options>
							</not_empty>
							<email_address>
								<name>email_address</name>
								<options>
									<messages>
										<emailAddressInvalidFormat><![CDATA[Invalid email address given.]]></emailAddressInvalidFormat>
									</messages>
								</options>
							</email_address>
						</validators>
					</email>
				</fsMain>
			</fieldsets>
		</inputFilter>
	</form>
</config>