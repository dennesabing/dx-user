<?xml version="1.0" encoding="UTF-8"?>
<config>
    <form>
		<name>FormName</name>
		<attributes>
			<method>post</method>
		</attributes>
		<fieldset>
			<name>fsMain</name>
			<type>Zend\Form\Fieldset</type>
			<options>
				<legend>Enter your registered email address</legend>
			</options>
		</fieldset>
		<fieldset>

		</fieldset>
		<element>
			<name>csrf</name>
			<type>Zend\Form\Element\Csrf</type>
		</element>
		<element>
			<name>email</name>
			<type>Zend\Form\Element\Email</type>
			<fieldset>fsMain</fieldset>
			<options>
				<label></label>
			</options>
		</element>
		<element>
			<name>submitBtn</name>
			<type>Zend\Form\Element\Submit</type>
			<options>
				<label>Submit</label>
				<primary>1</primary>
			</options>
			<attributes>
				<value>Submit</value>
			</attributes>
		</element>
	</form>
</config>